{
  "name": "react-cropper",
  "version": "1.3.0",
  "description": "Cropper as React components",
  "main": "dist/react-cropper.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./",
    "build": "babel ./src/react-cropper.js --out-file dist/react-cropper.js",
    "clean": "rimraf dist && mkdir dist",
    "prepublish": "npm run clean && npm run build",
    "prebuild": "npm run lint",
    "build-example": "npm run build && webpack --config webpack.config.js",
    "start": "node server.js"
  },
  "author": "Fong Kuanghuei <waneblade@gmail.com>",
  "homepage": "http://roadmanfong.github.io/react-cropper/",
  "bugs": {
    "url": "https://github.com/roadmanfong/react-cropper/issues"
  },
  "keywords": [
    "react",
    "react-component",
    "cropper",
    "image"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/roadmanfong/react-cropper.git"
  },
  "dependencies": {
    "babel-core": "7.0.0-bridge.0",
    "cropperjs": "^1.5.5",
    "prop-types": "^15.5.8"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^8.0.0-beta.6",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "css-loader": "^2.1.0",
    "eslint": "4.18.2",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "mini-css-extract-plugin": "^0.5.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^4.5.3",
    "rimraf": "^2.5.4",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      "transform-object-assign"
    ],
    "ignore": [
      "dist/**/*.js",
      "webpack.config.js",
      "webpack.config.dev.js",
      "bundle.js"
    ]
  }
}
